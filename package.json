{
  "name": "kick-api-types",
  "version": "1.0.11",
  "description": "Kick.com API Types",
  "author": "Sean Sattler <sean@sattler.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fb-sean/kick-api-types"
  },
  "keywords": [
    "kick",
    "api",
    "types"
  ],
  "exports": {
    "./v1": {
      "types": "./v1.d.ts",
      "import": "./v1.mjs",
      "require": "./v1.js"
    },
    "./payloads": {
      "types": "./payloads/index.d.ts",
      "import": "./payloads/index.mjs",
      "require": "./payloads/index.js"
    },
    "./payloads/v*": {
      "types": "./payloads/v*/index.d.ts",
      "import": "./payloads/v*/index.mjs",
      "require": "./payloads/v*/index.js"
    },
    "./rest": {
      "types": "./rest/index.d.ts",
      "import": "./rest/index.mjs",
      "require": "./rest/index.js"
    },
    "./rest/v*": {
      "types": "./rest/v*/index.d.ts",
      "import": "./rest/v*/index.mjs",
      "require": "./rest/v*/index.js"
    }
  },
  "files": [
    "{rest,payloads}/**/*.{js,js.map,d.ts,d.ts.map,mjs}"
  ],
  "scripts": {
    "clean": "rimraf --glob \"{payloads,rest}/**/*.{js,mjs,d.ts,*map}\" \"v*.{js,mjs,d.ts,*map}\"",
    "build": "runp clean lint:fix type :p \"build:*\"",
    "type": "tsc",
    "build:v1": "gen-esm-wrapper ./v1.js ./v1.mjs",
    "build:payloads": "gen-esm-wrapper ./payloads/index.js ./payloads/index.mjs",
    "build:rest": "gen-esm-wrapper ./rest/index.js ./rest/index.mjs",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@schummar/runp": "^2.2.4",
    "@types/node": "^22.13.10",
    "eslint": "^9.28.0",
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^6.1.0",
    "typescript": "^5.8.2"
  }
}
